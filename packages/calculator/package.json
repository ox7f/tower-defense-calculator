{
  "name": "@sf-girls-calculator/calculator",
  "author": "ox7f",
  "version": "0.0.1",
  "description": "math stuff",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint ./calculator/**/*",
    "typecheck": "tsc -b",
    "format": "prettier --write calculator"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "source": "calculator/index.ts",
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none",
    "printWidth": 120
